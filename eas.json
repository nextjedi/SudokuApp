{
  "cli": {
    "version": ">= 13.2.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "alpha": {
      "distribution": "internal",
      "channel": "alpha",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    },
    "beta": {
      "distribution": "internal",
      "channel": "beta",
      "android": {
        "buildType": "aab",
        "gradleCommand": ":app:bundleRelease"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    },
    "production": {
      "channel": "production",
      "android": {
        "buildType": "aab",
        "gradleCommand": ":app:bundleRelease"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production",
        "releaseStatus": "completed"
      },
      "ios": {}
    },
    "beta": {
      "android": {
        "track": "beta",
        "releaseStatus": "completed"
      },
      "ios": {}
    },
    "alpha": {
      "android": {
        "track": "internal",
        "releaseStatus": "completed"
      },
      "ios": {}
    }
  }
}
